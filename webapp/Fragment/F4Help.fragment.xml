<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:uitable="sap.ui.table">
	<Dialog title="{i18n>F4Dialog}" id="id_f4Dialog" class="cl_f4Help sapUiSizeCompact" verticalScrolling="false" contentWidth="500px"
		afterClose="fnAfterCloseFragment">
		<VBox class="sapUiSmallMargin cl_f4searchbox" width="80%" height="3rem">
			<SearchField liveChange="fnValueSearch" width="100%" placeholder="{i18n>SearchHere}" search="fnValueSearch" class="cl_f4searchfield"/>
		</VBox>
		<uitable:Table id="id_F4Table" visibleRowCountMode="Interactive" selectionBehavior="Row" rowSelectionChange="fnrowSelected"
			rows="{JM_F4Model>/rows}" selectionMode="Single" class="sapUiSmallMarginBottom cl_opr_Tbl_f4dialog"></uitable:Table>
		<endButton>
			<Button text="{i18n>Cancel}" class="sapUiSizeCompact cl_f4table_change cl_secondaryButton sapUiSmallMarginEnd"
				icon="{= ${JM_ImageModel>/path} + 'Cancel.svg'}" press="fnf4HelpCancel"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>